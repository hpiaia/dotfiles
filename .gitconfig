[user]
name = Humberto Piaia
email = betopiaia@gmail.com

[alias]
lg = log --all --graph --decorate --oneline --abbrev-commit
co = checkout
cm = commit
ac = !git add -A && git commit
st = status -sb
tags = tag -l
branches = branch -a
remotes = remote -v

[push]
default = current

[pull]
default = current
rebase = false

[init]
defaultBranch = main
